<?xml version='1.0' encoding='utf-8'?>
<widget id="com.publify.mx" version="1.0.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Publify</name>
    <description>
        Red de negocios
    </description>
    <author email="bernagaufo@hotmail.com" href="https://facebook.com/villbercreativo">
        Villber Guadalajara
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="https://viveenunclick.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="http://google.com" />
    <access origin="https://google.com" />
    <access origin="https://*.google.com" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" launch-external="yes" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <plugin name="cordova-plugin-googlemaps" source="npm" spec="2.1.0">
        <param name="API_KEY_FOR_ANDROID" value="AIzaSyBezcuO8jYhYFaelSbKgLBEANYR-1uW_-Q" />
        <param name="API_KEY_FOR_IOS" value="AIzaSyDCTVS77zM0c-fJaQILtEmTLzbhSrqeKO8" />
    </plugin>
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="SplashScreen" value="www/res/screen/android/screen-lpdi-portrait.png" />
        <preference name="SplashMaintainAspectRatio" value="true" />
        <preference name="SplashShowOnlyFirstTime" value="false" />
        <preference name="SplashScreenWidth" value="258" />
        <preference name="SplashScreenHeight" value="378" />
        <preference name="SplashScreenBackgroundColor" value="#ffffff" />
        <feature name="SocialSharing">
            <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
        </feature>
        <icon src="www/res/icon/android/icon-72-hdpi.png" />
        <icon density="ldpi" src="www/res/icon/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/res/icon/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/res/icon/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/res/icon/android/drawable-xhdpi-icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="res/ios/icon-60@3x.png" width="180" height="180" />
        <icon src="res/ios/icon-60.png" width="60" height="60" />
        <icon src="res/ios/icon-60@2x.png" width="120" height="120" />
        <icon src="res/ios/icon-76.png" width="76" height="76" />
        <icon src="res/ios/icon-76@2x.png" width="152" height="152" />
        <icon src="res/ios/icon-40.png" width="40" height="40" />
        <icon src="res/ios/icon-40@2x.png" width="80" height="80" />
        <icon src="res/ios/icon.png" width="57" height="57" />
        <icon src="res/ios/icon@2x.png" width="114" height="114" />
        <icon src="res/ios/icon-72.png" width="72" height="72" />
        <icon src="res/ios/icon-72@2x.png" width="144" height="144" />
        <icon src="res/ios/icon-small.png" width="29" height="29" />
        <icon src="res/ios/icon-small@2x.png" width="58" height="58" />
        <icon src="res/ios/icon-small@3x.png" />
        <icon src="res/ios/icon-50.png" width="50" height="50" />
        <icon src="res/ios/icon-50@2x.png" width="100" height="100" />
        <feature name="SocialSharing">
            <param name="ios-package" value="SocialSharing" />
            <preference name="Orientation" value="portrait" />
        </feature>
    </platform>
    <preference name="Orientation" value="portrait" />
    <preference name="loadUrlTimeoutValue" value="700000"/>
    <preference name="SplashScreen" value="screen" />
    <splash src="www/res/screen/android/screen-lpdi-portrait.png" />
    <engine name="android" spec="6.2.3" />
    <engine name="ios" spec="4.4.0" />
    <plugin name="cordova-plugin-geolocation" spec="2.4.3">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-network-information" spec="1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="4.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-x-socialsharing" spec="5.2.0" />
</widget>
